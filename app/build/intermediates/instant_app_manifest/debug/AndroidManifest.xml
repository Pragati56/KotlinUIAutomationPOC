<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sampleapplication"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sampleapplication.Main3Activity"
            android:label="@string/title_activity_main3"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.example.sampleapplication.EnteratinmentRoom"
            android:label="Welcome to my Enteratinment Room " >
        </activity>
        <activity
            android:name="com.example.sampleapplication.Information"
            android:label="Update your self with latest information" >
        </activity>
        <activity
            android:name="com.example.sampleapplication.KitchenMain3Activity"
            android:label="Welcome to my Kitchen" >
        </activity>
        <activity
            android:name="com.example.sampleapplication.MyMedicine"
            android:label="Store some basic medicines" >
        </activity>
        <activity
            android:name="com.example.sampleapplication.MyEntertainment"
            android:label="@string/title_activity_enteratinment_room" >
        </activity>
        <activity
            android:name="com.example.sampleapplication.LaunchActivity"
            android:label="Covid-19 Precautions" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>